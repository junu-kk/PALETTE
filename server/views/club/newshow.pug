doctype html

head
  include ../includes/head.pug

.wrapper
  include ../includes/sidebar.pug
  .main-panel
    include ../includes/navbar.pug
    .content
      block content
        .row
          .col-lg-3.col-md-5
            .card
              .card-content
                -if(!(ct.club.pic==null||ct.club.pic==''))
                  .image-center
                    img(src=ct.club.pic alt='')
                
                h3 #{ct.club.name}
                h5 #{ct.club.info}

include ../includes/scripts.pug